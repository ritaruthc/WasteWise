{
    "version": 2,
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}