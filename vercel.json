{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php@0.6.2"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}