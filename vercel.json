{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|storage|icon.png|favicon.ico)(.*)",
      "dest": "/public/$1$2"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/public/index.php"
    }
  ]
}